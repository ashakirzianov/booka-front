{
    "files.exclude": {
        "**/build/**": true,
        "**/node_modules/**": true
    },
    "typescript.preferences.importModuleSpecifier": "relative",
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
    },
    "editor.formatOnSave": true,
    "cSpell.words": [
        "DELIM",
        "Helvetica",
        "Hoverable",
        "Interop",
        "Kapital",
        "Metas",
        "Newable",
        "Perc",
        "Refable",
        "Scrollable",
        "Shakirzianov",
        "Subnodes",
        "Subpart",
        "Syncable",
        "Themeable",
        "Themer",
        "Unmount",
        "Whitespaces",
        "actionize",
        "backend",
        "booka",
        "browserslist",
        "collapsable",
        "eofline",
        "esnext",
        "homescreen",
        "jssdk",
        "middlewares",
        "nums",
        "onstatechange",
        "onupdatefound",
        "polyfill",
        "precached",
        "promisify",
        "quotemark",
        "refability",
        "shinel",
        "subpath",
        "succ",
        "timeouted",
        "webfonts",
        "webview",
        "xfbml"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.wordWrap": "bounded",
    "editor.wrappingIndent": "indent",
    "bracketPairColorizer.showVerticalScopeLine": false,
    "bracketPairColorizer.showHorizontalScopeLine": false
}